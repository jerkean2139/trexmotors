{
  "version": 2,
  "buildCommand": "mkdir -p dist && cp -r server/public dist/",
  "outputDirectory": "dist/public",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}