{
  "version": 2,
  "buildCommand": "echo 'Skipping build - using pre-built static files'",
  "outputDirectory": "server/public",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}