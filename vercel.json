{
  "version": 2,
  "builds": [
    {
      "src": "server/public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/vehicles.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vehicles",
      "dest": "/api/vehicles.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/public/$1"
    }
  ]
}