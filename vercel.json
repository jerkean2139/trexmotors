{
  "version": 2,
  "installCommand": "echo 'Skipping npm install'",
  "buildCommand": "echo 'No build process - static deployment only'",
  "outputDirectory": "server/public",
  "functions": {
    "api/vehicles.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/vehicles",
      "dest": "/api/vehicles.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}